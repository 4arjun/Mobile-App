{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "SUPABASE_URL": "@SUPABASE_URL",
        "SUPABASE_KEY": "@SUPABASE_API"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "SUPABASE_URL": "@SUPABASE_URL",
        "SUPABASE_KEY": "@SUPABASE_API"
      }

    },
    "production": {
      "autoIncrement": true,
      "env": {
        "SUPABASE_URL": "@SUPABASE_URL",
        "SUPABASE_KEY": "@SUPABASE_API"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
